@model IEnumerable<FinalProgamacion3.Models.Entradas>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Proveedores.Nombre)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Fecha)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Cantidad)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IDProductos)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Proveedores.Nombre)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Fecha)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Cantidad)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IDProductos)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.IDEntradas }) |
                @Html.ActionLink("Details", "Details", new { id = item.IDEntradas }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.IDEntradas })
            </td>
        </tr>
    }

</table>


